{
	"name": "vscode-autohotkey-plus-plus",
	"displayName": "AutoHotkey Plus Plus",
	"description": "AutoHotkey IntelliSense, debug, and language support for VS Code, forked from AutoHotkey Plus by cweijan",
	"version": "2.8.1",
	"publisher": "mark-wiemer",
	"engines": {
		"vscode": "^1.30.0"
	},
	"activationEvents": [
		"onLanguage:ahk"
	],
	"main": "./out/extension",
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"license": "MIT",
	"icon": "icon.png",
	"bugs": {
		"url": "https://github.com/mark-wiemer/vscode-autohotkey-plus-plus/issues"
	},
	"homepage": "https://github.com/mark-wiemer/vscode-autohotkey-plus-plus/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/mark-wiemer/vscode-autohotkey-plus-plus.git"
	},
	"galleryBanner": {
		"color": "#00769D",
		"theme": "dark"
	},
	"keywords": [
		"vscode",
		"autohotkey",
		"ahk"
	],
	"contributes": {
		"breakpoints": [
			{
				"language": "ahk"
			}
		],
		"debuggers": [
			{
            
			}
		],
		"languages": [
			{
				"id": "ahk",
				"aliases": [
					"AutoHotkey",
					"autohotkey",
					"ahk"
				],
				"extensions": [
					".ahk",
					".ahk1",
					".ahk2",
					".ah2",
					".ext"
				],
				"configuration": "./ahk.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ahk",
				"scopeName": "source.ahk",
				"path": "./syntaxes/ahk.tmLanguage.json"
			}
		],
		"commands": [

		],
		"menus": {
		},
		"keybindings": [
		],
		"configuration": {
			"type": "object",
			"title": "AutoHotkey Plus Plus",
			"properties": {
				"ahk++.enableIntellisense": {
					"type": "boolean",
					"default": true,
					"description": "Enable IntelliSense (Preview). Changes take effect after reload."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"tslint": "tslint -t verbose src/**/*.ts"
	},
	"dependencies": {
		"get-port": "^5.1.1",
		"vscode-debugadapter": "^1.44.0",
		"xml2js": "^0.4.23"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.7",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.1",
		"@types/node": "^14.14.31",
		"@types/vscode": "^1.30.0",
		"fs-extra": "^9.1.0",
		"glob": "^7.1.6",
		"mocha": "^8.3.0",
		"source-map-support": "^0.5.19",
		"tslint": "^5.15.0",
		"typescript": "^3.4.3",
		"vscode-test": "^1.5.1"
	},
	"__metadata": {
		"id": "cf96e3c7-3b07-4403-acc9-2b6299ba7877",
		"publisherId": "5d0c3a99-7c1e-41ac-a4b0-401d0091adcf",
		"publisherDisplayName": "Mark Wiemer",
		"isPreReleaseVersion": false,
		"preRelease": false,
		"installedTimestamp": 1648289698691
	}
