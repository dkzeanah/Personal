nt|(?:controlgetclassn|hidedropdow|s(?:h(?:owdrop|ut)dow|trle|i)|fileope|a(?:si|ta)|mi)n|(?:monitorgetprimar|s(?:e(?:t(?:control|mouse|key|win)de|ndp)|oundp)la|regdeleteke|(?:comobjque|keyhisto)r|comobjarra|il_destro|(?:file|dir)cop|hotke)y|varsetstrcapacity|(?:editgetcurrentco|(?:drivegetseri|critic)a|(?:drive[gs]et|is)labe|sendleve)l|(?:controlg|s)etenabled|in(?:st(?:all(?:keybd|mouse)hook|r)|puthook)|filerecycleempty|(?:driveg|obj[gs])etcapacity|(?:drivegetstatusc|(?:GuiCtrlFromH|controlgeth|guifromh)wn|o(?:bjbindmetho|r)|(?:(?:s(?:endtexts|usp)|fileapp)e|rou)n|(?:get|has)metho|filerea|groupad|(?:down|re)loa|datead|(?:ini|reg)rea|il_ad|threa)d|(?:editgetline|monitorget|objownprop)count|(?:process(?:wait)?|group)close|drivegetstatus|file(?:setattr|getatt)rib|(?:mouseclickdra|outputdebu)g|filegetversion|(?:objhasown|has)prop|(?:comobjfrompt|s(?:et(?:workingdi|time)|tr(?:lowe|uppe|pt)|ubst)|(?:integ|buff|numb)e|floo|ch)r|pixelgetcolor|(?:(?:c(?:ontrolc)?li|drive(?:un)?lo)c|getkeyv)k|(?:(?:c(?:lipboard|omc)|dllc)a|fileinsta|ce)ll|e(?:dit(?:getline)?|xit)|f(?:ile(?:recycle|get)|ormat)|processwait|obj(?:ownprops|addref)|t(?:raysetico|a)n|(?:(?:image|pixel)sear|regexmat)ch|mouseclick|setregview|(?:(?:obj(?:rele|[gs]etb)|hasb)a|pau)se|r(?:egdelete|un)|splitpath|(?:s(?:oundb|l)ee|exitap|t(?:ool|ray)ti)p|s(?:e(?:nd(?:text)?|t)|ysget|how)|getkeysc|datediff|issetref|control|onerror|string|random|isset|hide|goto|gui|cos|exp|l(?:og|n))\\b",
                  "name": "support.function.builtin.ahk"
               },
               {
                  "match": "\\b(\\w)+(?=\\()",
                  "name": "support.function.ahk",
                  "example": "fun("
               },
               {
                  "match": "[\\s\\(]+\"http(s)?:\\/\\/.+\"[\\s\\)]+",
                  "name": "link.string.ahk",
                  "example": "\"https://github.com/Axlefublr/Personal/blob/main/_Libraries/Func/3.%20Global.ah2\""
               },
               {
                  "begin": "(\")",
                  "beginCaptures": {
                     "1": {
                           "name": "string.start.ahk"
                     }
                  },
                  "end": "(\")",
                  "endCaptures": {
                     "1": {
                           "name": "string.end.ahk"
                     }
                  },
                  "patterns": [
                     {
                           "match": "`\"",
                           "name": "punctuation.ahk"
                     }
                  ],
                  "name": "string.ahk",
                  "example": "\"string\""
               },
               {
                  "match": "\\b(?<!\\.)(?i:k|key|v|value|i|index|text|(\\w+)title|(\\w+)path|(loc(\\d?)[XY])|(pix(\\d?)[XY]))\\b",
                  "name": "keyword.other.ahk",
                  "example": "text"
               },
               {
                  "match": "\\w+",
                  "name": "variable.def.ahk"
               }
         ]
      }
   }
}
