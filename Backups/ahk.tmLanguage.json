{
   "fileTypes": ["ahk"],
   "name": "AutoHotkey",
   "scopeName": "source.ahk",
   "patterns": [
      {
         "include": "#comment"
      },
      {
         "include": "#string"
      },
      {
         "include": "#script-block"
      },
      {
         "include": "#function_call"
      },
      {
         "include": "#keyword"
      },
      {
         "include": "#punctuation"
      },
      {
         "include": "#text"
      }
   ],
   "repository": {
      "comment": {
         "patterns": [
               {
                  "captures": {
                     "1": {
                           "name": "comment.ahk"
                     }
                  },
                  "match": "\\s*(;.*)",
                  "example": ";comment"
               },
               {
                  "begin": "^\\s*/\\*",
                  "end": "^\\s*\\*/",
                  "name": "comment.ahk",
                  "example": "/* comment */"
               }
         ]
      },
      "string": {
         "patterns": [
               {
                  "begin": "\"[ \\t]*\\n\\s*\\(",
                  "end": "\\)\\s*\"",
                  "name": "string.continuation.ahk",
                  "comment": "Quirked up white boy busts it down sexual style. Is he goated with the sauce?"
               }
         ]
      },
      "script-block": {
         "patterns": [
               {
               "match": "\\b(?i:C_\\w+)\\(",
                  "name": "support.class.ahk",
                  "example": "classInst := C_ClassName()"
               },
               {
                  "match": "\\b(?<=\\.)(?i:focusedctrl|is(?:variadic|builtin)|(?:c(?:lickc)?oun|tex)t|(?:backcolo|pt)r|m(?:inparams|a(?:xparams|rgin[xy])|enubar)|(?:casesens|(?:hand|tit)l|nam|siz)e|v(?:aluetyp|isibl)e|en(?:coding|abled)|capacity|focused|classnn|default|__item|length|result|value|ateof|hwnd|gui|pos|type)\\b",
                  "name": "support.class.builtin.property.ahk",
                  "example": ".length"
               },
               {
                  "match": "\\b(?<=\\.)(?i:(?:(?:toggleen|dis|en)abl|write(?:doubl|lin)|read(?:doubl|lin)|prototyp|m(?:(?:ax|in)imiz|ov)|r(?:awwrit|e(?:stor|nam))|delet|hid)e|get(?:client)?pos|(?:toggle|un)?check|(?:addstandar|rawrea)d|(?:write(?:u?shor|floa)|read(?:u?shor|floa)|(?:writeui|readui|oneve)n|(?:remove|form)a|s(?:etfon|ubmi)|op)t|writeint64|(?:(?:writeu?|readu?)ch|cle)ar|i(?:s(?:optional|byref)|nsert(?:at)?)|readint64|write(?:int)?|onnotify|s(?:e(?:t(?:color|icon)|ek)|how)|read(?:int)?|destroy|__enum|usetab|c(?:hoos|lo[ns])e|(?:redra|__ne)w|flash|(?:focu|ha)s|p(?:ush|op)|bind|call|add|get|set)(?=\\()\\b",
                  "name": "support.class.builtin.method.ahk",
                  "example": ".length"
               },
               {
                  "match": "\\b\\w+(?=\\.[%]?\\w+[%]?)",
                  "name": "support.class.ahk",
                  "example": "classInst.Method()"
               },
               {
                  "captures": {
                     "1": {
                           "name": "hotkey.label.definition.ahk"
                     },
                     "2": {
                           "name": "hotkey.label.hotkey.ahk"
                     },
                     "3": {
                           "name": "hotkey.label.definition.ahk"
                     }
                  },
                  "match": "(:[bceiknoprsxzBCEIKNOPRSXZ0?\\*]{1,5}:)(.+)(::)",
                  "example": "::hot::"
               },
               {
                  "captures": {
                     "1": {
                           "name": "hotkey.label.hotkey.ahk"
                     },
                     "2": {
                           "name": "hotkey.label.definition.ahk"
                     }
                  },
                  "match": "^\\s*(.+)(::)",
                  "example": "^c::"
               }
            ]
         },
         "function_call": {
            "patterns": [
               {
                  "captures": {
                     "1": {
                        "name": "directives.parameters.ahk"
                     },
                     "2": {
                        "name": "comment.ahk"
                     }
                  },
                  "match": "#\\b\\w*(?!:{1,2})\\b([^;]*)(\\s+;.*)?$",
                  "name": "keyword.control.directives.ahk",
                  "example": "#persistent"
               },
               {
                  "match": "\\b(?i:a_[a-z,A-Z]+)\\b",
                  "name": "variable.builtin.ahk",
                  "example": "A_ScriptDir"
               }
            ]
         },
         "keyword": {
            "patterns": [
               {
                  "captures": {
                     "1": {
                           "name": "keyword.control.ahk"
                     },
                     "2": {
                           "name": "support.class.ahk"
                     },
                     "4": {
                           "name": "keyword.control.ahk"
                     },
                     "5": {
                           "name": "support.class.ahk"
                     }
                  },
                  "match": "(?i:\\b(class)\\b\\s*(\\w+))\\s*((extends)\\s+(\\w+))?",
                  "example": "Class class1 extends class2"
               },
               {
                  "match": "\\b(?i:if|else|switch|case|return|loop|break|for|in|while|catch|finally|throw|try|until|continue|true|false|default|except)\\b",
                  "name": "keyword.control.ahk"
               },
               {
                  "match": "(?x) \\b\r\n((0(x|X)[0-9a-fA-F]*)\r\n|(\r\n  ([0-9]+\\.?[0-9]*)\r\n  |(\\.[0-9]+)\r\n )((e|E)(\\+|-)?[0-9]+)?\r\n)\\b\r\n",
                  "name": "constant.numeric.ahk"
               },
               {
                  "match": "[\\/|:*+-.~\\^]\\/?=",
                  "name": "keyword.operator.assignment.ahk"
               },
               {
                  "match": "[/+\\-*\\^#]",
                  "name": "keyword.operator.arithmetic.ahk"
               },
               {
                  "match": "[<>=!|]|[<>=]=|!(={1,2})|(?i:or|and|not)|&&",
                  "name": "keyword.operator.comparison.ahk"
               },
               {
                  "match": "=>|[%?&:]",
                  "name": "keyword.operator.varref.ahk"
               }
         ]
      },
      "punctuation": {
         "patterns": [
               {
                  "match": "[`.,]",
                  "name": "punctuation.ahk"
               },
               {
                  "match": "[\\[\\](){}]",
                  "name": "punctuation.bracket.ahk"
               }
         ]
      },
      "text": {
         "patterns": [
